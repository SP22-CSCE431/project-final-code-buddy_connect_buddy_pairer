            <%= check_box_tag 'interest[pastWorkExp][]', 'Other', checked('Other'), id:'pastWorkExp_other'%>
            Other<br>
            
        </div>
      </div>
    </div>

    <div>
      <div class="actions", style="float:right;">
        <%= form.submit "Create Interest", :class => 'btn btn-outline-dark'%>
      </div>

      <%= link_to 'Back', interests_path, class:"btn btn-outline-dark" %>

    </div>

    &nbsp;&nbsp;
    
  <% end %>
</div>

