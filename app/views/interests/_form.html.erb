            <%= check_box_tag 'interest[potentialRoles][]', 'Business Analytics', checked('Business Analytics'), id:'potentialRoles_businessA'%>
            Business Analytics<br>

            <%= check_box_tag 'interest[potentialRoles][]', 'Cyber Security', checked('Cyber Security'), id:'potentialRoles_cyberS'%>
            Cyber Security<br>

        </div>  

        <div class="field", style="width:200px;float:right;">
          <b><%= form.label :Select_fields_you_have_worked_in %></b>

          <%= check_box_tag 'interest[pastWorkExp][]', 'Software Development', checked('Software Development'), id:'pastWorkExp_softwareDev'%>
            Software Development<br>

            <%= check_box_tag 'interest[pastWorkExp][]', 'Data Analytics and Visualization', checked('Data Analytics and Visualization'), id:'pastWorkExp_dataAnV'%>
            Data Analytics and Visualization<br>

            <%= check_box_tag 'interest[pastWorkExp][]', 'Data Science', checked('Data Science'), id:'pastWorkExp_dataSc'%>
            Data Science<br>


            <%= check_box_tag 'interest[pastWorkExp][]', 'Consulting', checked('Consulting'), id:'pastWorkExp_consult'%>
            Consulting<br>

            <%= check_box_tag 'interest[pastWorkExp][]', 'Systems Analyst', checked('Systems Analyst'), id:'pastWorkExp_systemA'%>
            Systems Analyst<br>

            <%= check_box_tag 'interest[pastWorkExp][]', 'Project Management', checked('Project Management'), id:'pastWorkExp_projectM'%>
            Project Management<br>

            <%= check_box_tag 'interest[pastWorkExp][]', 'Product Management', checked('Product Management'), id:'pastWorkExp_productM'%>
            Product Management<br>

            <%= check_box_tag 'interest[pastWorkExp][]', 'Data Engineering/ETL Developer', checked('Data Engineering/ETL Developer'), id:'pastWorkExp_dataEngETL'%>
            Data Engineering/ETL Developer<br>

            <%= check_box_tag 'interest[pastWorkExp][]', 'Business Analytics', checked('Business Analytics'), id:'pastWorkExp_businessA'%>
            Business Analytics<br>

            <%= check_box_tag 'interest[pastWorkExp][]', 'Cyber Security', checked('Cyber Security'), id:'pastWorkExp_cyberS'%>
